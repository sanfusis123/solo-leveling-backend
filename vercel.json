{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "pip_install_command": "pip install -r requirements.txt"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", 
    "dest": "app/main.py" 
   }
  ]
}
